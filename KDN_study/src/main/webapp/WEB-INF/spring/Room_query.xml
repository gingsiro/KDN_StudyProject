<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 

<mapper namespace="room">
	<select id="search" parameterType="int" resultType="room">
		select *
		from room
		where rno = 101
	</select>
	
	<select id="rsvRoomSearch" parameterType="String" resultType="RsvRoom">
		select *
		from rsv_room
		where rsvdate = to_date(#{roomdate}, 'yyyy-mm-dd')
	</select>

</mapper>